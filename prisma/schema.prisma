generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("POSTGRES_URL") // uses connection pooling
  shadowDatabaseUrl = env("POSTGRES_URL")
}

model Link {
  id      String @id
  html    String
  shapeId String @unique @map("shape_id")

  @@map("links")
}
